{
  "name": "bpmn-elements",
  "version": "0.0.1",
  "description": "Executable workflow elements based on BPMN 2.0",
  "main": "dist/index.js",
  "module": "index.js",
  "scripts": {
    "test": "mocha -R dot",
    "posttest": "eslint . --cache && npm run dist",
    "prepare": "npm run dist",
    "cov:html": "nyc mocha -R dot && nyc report --reporter=html",
    "dist": "babel index.js -d dist && rm -rf dist/src && babel src -d dist/src"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/bpmn-elements"
  },
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "license": "MIT",
  "keywords": [
    "bpmn",
    "bpmn 2",
    "workflow",
    "automation"
  ],
  "nyc": {
    "exclude": [
      "dist",
      "test"
    ]
  },
  "files": [
    "dist",
    "src",
    "index.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@bonniernews/wichita": "^0.5.0",
    "bpmn-moddle": "^5.1.6",
    "camunda-bpmn-moddle": "^3.2.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "eslint": "^5.16.0",
    "mocha": "^6.1.2",
    "mocha-cakes-2": "^3.3.0",
    "moddle-context-serializer": "^0.1.1",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "request": "^2.88.0"
  },
  "dependencies": {
    "iso8601-duration": "^1.2.0",
    "smqp": "^1.3.0"
  }
}
